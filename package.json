{
  "name": "cypressletsgo",
  "version": "1.0.0",
  "description": "cypress with cucumber with letsgo friends",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/cypress run",
    "test_put_folder": "./node_modules/.bin/cypress run --spec cypress/e2e/features/put",
